//2a exercise 2


void setup(){
   size(500,400);
   background(0);
}


void draw(){
  if (mouseButton == LEFT)  //press left many 50pi red circle with white outline 
   {  fill(#A00A14);
     stroke(255);
     circle(mouseX,mouseY,50);
   }
   
   if(mouseButton == RIGHT)
   {
    fill(#39D7EA);
    stroke(255);
    rect(mouseX,mouseY,50,50);
   
   }
   
   
 
}
